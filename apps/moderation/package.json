{
	"name": "moderation",
	"version": "0.1.0",
	"description": "",
	"scripts": {
		"build:clean": "del-cli dist",
		"build:check": "tsc --noEmit",
		"build:esm": "swc ./src --out-dir ./dist",
		"build": "yarn build:clean && yarn build:check && yarn build:esm",
		"start": "env-cmd node --enable-source-maps ./dist/index.js",
		"dev:framework": "yarn workspace @moderation/framework run build && yarn build && yarn start",
		"start-build": "yarn build && yarn start",
		"dev": "tsc-watch --onSuccess \"yarn workspace moderation run start-build\""
	},
	"type": "module",
	"keywords": [],
	"author": "WoozyStudio <woozystudiocontact@gmail.com>",
	"license": "UNLICENSED",
	"dependencies": {
		"@moderation/framework": "workspace:^",
		"discord.js": "14.21.0",
		"dotenv": "17.2.1",
		"env-cmd": "10.1.0",
		"mongoose": "8.18.0",
		"path": "0.12.7",
		"tsc-watch": "7.1.1"
	},
	"devDependencies": {
		"@swc/cli": "0.1.63",
		"@swc/core": "1.3.100",
		"@types/node": "24.1.0",
		"typescript": "5.8.3"
	},
	"engines": {
		"node": ">=20.18.3"
	},
	"publishConfig": {
		"access": "public"
	}
}
