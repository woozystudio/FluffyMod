		"@naval-base/ms": "3.1.0",
		"botten": "0.1.1",
		"discord.js": "14.22.1",
		"dotenv": "17.2.1",
		"env-cmd": "10.1.0",
		"mongoose": "8.18.0",
		"path": "0.12.7",
		"tsc-watch": "7.1.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.0",
		"@swc/cli": "0.1.63",
		"@swc/core": "1.3.100",
		"@types/node": "24.1.0",
		"typescript": "5.8.3"
